<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.KeshifenleiDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.KeshifenleiEntity" id="keshifenleiMap">
        <result property="keshifenlei" column="keshifenlei"/>
        <result property="image" column="image"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.KeshifenleiVO" >
		SELECT * FROM keshifenlei  keshifenlei         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.KeshifenleiVO" >
		SELECT  keshifenlei.* FROM keshifenlei  keshifenlei 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.KeshifenleiView" >

		SELECT  keshifenlei.* FROM keshifenlei  keshifenlei 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.KeshifenleiView" >
		SELECT * FROM keshifenlei  keshifenlei <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
